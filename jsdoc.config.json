
{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false, 
        "default":{
            "outputSourceFiles": false
        }
    },
    "opts": {
        "template": "templates/default",  
        "encoding": "utf8",               
        "destination": "./documentation/",          
        "recurse": true             
    }
}

